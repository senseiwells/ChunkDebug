{
  "schemaVersion": 1,
  "id": "chunk-debug",
  "version": "${version}",
  "name": "ChunkDebug",
  "description": "Chunk Debug server side support",
  "authors": [
	"Sensei"
  ],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.senseiwells.chunkdebug.ChunkDebug",
      "me.senseiwells.chunkdebug.server.ChunkDebugServer"
    ],
    "client": [
      "me.senseiwells.chunkdebug.client.ChunkDebugClient"
    ]
  },
  "mixins": [
    "chunk-debug.server.mixins.json",
    "chunk-debug.client.mixins.json"
  ],
  "accessWidener": "chunk-debug.accesswidener",
  "depends": {
    "minecraft": "${minecraft_dependency}",
    "fabricloader": ">=${fabric_loader_dependency}",
    "fabric-api": "*"
  }
}
